{
    "version": "0.2.0",
    "configurations": [
        {
            "name":"Launch",
            "type": "node",
            "request": "launch",
            "args": ["${workspaceFolder}/src/main.ts"],
            "runtimeArgs": ["--no-lazy", "-r", "ts-node/register", "-r", "tsconfig-paths/register"],
            "cwd": "${workspaceRoot}",
            "internalConsoleOptions": "openOnSessionStart",
            "sourceMaps": true,
            "console": "internalConsole",
            "outputCapture": "std"
        }
    ]
}